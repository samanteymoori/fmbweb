<h3>Icd10 Lookup</h3>
<br />
<form asp-controller="Icd10" asp-action="Search" id="frm" data-parsley-validate class="form-horizontal form-label-left" method="post" enctype="multipart/form-data">
    <div style="width:800px">

        <div class="form-group">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Description</label>
            <div class="col-md-9 col-sm-9 col-xs-12">
                <input type="text" name="Description" id="Description" value="" class="form-control col-md-10" autocomplete="off">
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-9 col-sm-9 col-xs-12">
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <table style="width:500px;margin-left:200px">
                        <tr>
                            <td style="width:300px"><button type="submit" style="width:90%" class="btn btn-info btn-lg">&nbsp;&nbsp;&nbsp;Search&nbsp;&nbsp;&nbsp;</button></td>
                            <td><button type="button" onclick="window.location=window.location" style="width:90%" class="btn btn-info btn-lg">Reset Form</button></td>
                        </tr>
                    </table>


                </div>
            </div>
        </div>
    </div>
</form>